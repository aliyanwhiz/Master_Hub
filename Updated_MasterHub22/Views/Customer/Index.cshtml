@using DashBoard2.Models;
@{
	Layout = "../shared/_Layoutwebsite.cshtml";
}
@model List<Services>
<!-- Hero Section -->

<style>
	.feature-img{
		width : 350px !important;
		height : 250px !important;
	}
</style>
<section class="hero-section-two">
	<div class="banner-slider slider">
		<div class="banner">
			<img class="img-fluid" src="~/website/img/banner.jpg" alt="img">
		</div>
		<div class="banner">
			<img class="img-fluid" src="~/website/img/banner-02.jpg" alt="img">
		</div>
		<div class="banner">
			<img class="img-fluid" src="~/website/img/banner-03.jpg" alt="img">
		</div>
	</div>
	<div class="banner-search aos" data-aos="fade-up">
		<div class="container">
			<div class="row">
				<div class="col-md-7 mx-auto">
					<h1>Multiple Services<span> MasterHub</span></h1>
					<p>Multiple Services MasterHub</p>
					<div class="col-md-5 mx-auto">
						<div class="search-box-two  content-center">
							<div class="search-btn w-100">
								<a href="@Url.Action("AllServices","Customer")"><button class="btn search_service" type="submit">See All Services</button></a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<!-- /Hero Section -->
<!-- Feature Section -->
<section class="feature-section-two">
	<div class="container">
		<div class="row">
			<div class="col-md-6 aos" data-aos="fade-up">
				<div class="section-heading-two">
					<h2>Multiple Services</h2>
					<p>What do you need to find?</p>
				</div>
			</div>
			<div class="col-md-6 text-md-end aos" data-aos="fade-up">
				<a href="@Url.Action("AllServices","Customer")" class="btn btn-pink btn-viewall">View All<i class="feather-arrow-right-circle"></i></a>
			</div>
		</div>
		<div class="row">
			
			@foreach (var dataimg in Model)
			{
				<div class="col-md-6 col-lg-4">
					<div class="feature-widget">
						<div class="feature-img">
							<a href="@Url.Action("ServiceBooking","Customer",new {myid= dataimg.Service_Id})">
								<img src="@("~/service_image/"+dataimg.Services_Image)" asp-append-version="true" class="" alt="img">
							</a>
						</div>
						<a href="@Url.Action("ServiceBooking","Customer",new {myid= dataimg.Service_Id})">
							<div class="feature-icon">
								<span>
									<img src="~/website/img/icons/feature-icon-09.svg" alt="img">
								</span>
								<div class="feature-title">
									<h5>@dataimg.Services_Name</h5>
								</div>
							</div>
						</a>
					</div>
				</div>
			}
		</div>
	</div>
</section>
<!-- /Feature Section -->
<!-- Work Section -->
